  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "firebase": "^12.0.0",
    "zustand": "^4.0.0 || ^5.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0",
    "lucide-react": "^0.400.0",
    "recharts": "^2.0.0 || ^3.0.0",
    "react-hot-toast": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "firebase": "^12.8.0",
    "zustand": "^5.0.10",
    "react-router-dom": "^7.12.0",
    "lucide-react": "^0.562.0",
    "recharts": "^3.6.0",
    "react-hot-toast": "^2.6.0",
    "tsup": "^8.0.1",
    "typescript": "~5.9.3"
  },
  "keywords": [
