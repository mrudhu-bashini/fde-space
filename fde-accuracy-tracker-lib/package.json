{
  "name": "@fde/accuracy-tracker",
  "version": "1.0.0",
  "description": "Reusable FDE Accuracy Tracker library for tracking AI/Agent accuracy issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
